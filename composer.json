{
    "name": "kigamba/laravel-tinker-tools",
    "description": "Use short class names in an Artisan tinker session -> Supports php 5",
    "keywords": [
        "spatie",
        "laravel-tinker-tools",
        "flysystem",
        "dropbox",
        "v2",
        "api"
    ],
    "homepage": "https://github.com/Kigamba/laravel-tinker-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Ephraim Kigamba, Freek Van der Herten",
            "email": "n.kigamba@students.ku.ac.ke, freek@spatie.be",
            "homepage": "https://uzapos.com, https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.0",
        "illuminate/support": "5.3.*|5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TinkerTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TinkerTools\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
