{
    "name": "kigamba/laravel-tinker-tools",
    "description": "Use short class names in an Artisan tinker session -> Supports php 5",
    "keywords": [
        "laravel-tinker-tools",
        "flysystem"
    ],
    "homepage": "https://github.com/Kigamba/laravel-tinker-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Ephraim Kigamba",
            "email": "kigamba@uzapos.com",
            "homepage": "https://uzapos.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.3.*|5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TinkerTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TinkerTools\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
